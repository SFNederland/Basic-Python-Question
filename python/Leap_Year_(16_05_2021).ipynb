      "source": [
        "year = int(input(\"Please enter a year: \"))\n",
        "if year % 400 == 0 or ( year % 4 == 0 and year % 100 != 0):\n",
        "    print(f\"{year} is a leap year\")\n",
        "else:\n",
        "    print(f\"{year} is not a leap year\")"
      ],
      "execution_count": 2,
